public class Escola {
    String nome;
    String endereco;
    int numeroAlunos;
    String serie;

    public Escola(String nome, String endereco, int numeroAlunos, String serie) {
        this.nome = nome;
        this.endereco = endereco;
        this.numeroAlunos = numeroAlunos;
        this.serie = serie;
    }

    public void iniciarAula() {
        System.out.println("Aula iniciada!");
    }

    public void encerrarAula() {
        System.out.println("Aula encerrada.");
    }

    public void aplicarProva() {
        System.out.println("Prova aplicada com sucesso!");
    }

    public void corrigirProva() {
        System.out.println("Provas corrigidas!");
    }
}
